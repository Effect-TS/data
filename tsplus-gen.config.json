{
  "project": {
    "packageName": "@fp-ts/data",
    "baseDir": "src",
    "tsconfig": "tsconfig.base.json",
    "paths": ["!**/internal/**"],
    "staticPrefixes": ["make", "from", "unsafeFrom"],
    "getterNamespaces": ["fp-ts/data", "ReadonlyArray"]
  },
  "namespaces": [
    {
      "name": "fp-ts/data",
      "type": {
        "include": true,
        "includeCompanion": true
      }
